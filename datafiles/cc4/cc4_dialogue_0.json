    {
      "sprite": "AllieDefault",
      "sound": "Allie",
      "delay": 0,
      "text": "How's your field trip?",
      "shake": false
    },
    {
      "sprite": "ArtyHappy",
      "sound": "Arty",
      "delay": 0,
      "text": "Going well! I'm not really a geology \nexpert, but it's been fascinating \nlearning about the formation \nand properties of these \nexotic crystals.",
      "shake": false
    },
    {
      "sprite": "AllieDefault",
      "sound": "Allie",
      "delay": 0,
      "text": "Yeah. You're in the Graveyard, \nright?",
      "shake": false
    },
    {
      "sprite": "ArtyDefault",
      "sound": "Arty",
      "delay": 0,
      "text": "Yes, also known as the \nWadegobi Desert.",
      "shake": false
    },
    {
      "sprite": "AllieCautious",
      "sound": "Allie",
      "delay": 0,
      "text": "Sounds creepy. Be careful...",
      "shake": false
